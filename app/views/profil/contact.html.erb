<div class="flex">
  <div class="scroll">
    <div class="list">
      <%= render 'shared/list'%>
    </div>
  </div>
   <div class="show">
    <div class="container">
      <div class="work">
        <div class="entete"><h1>Contact</h1></div>
        <% @contacts.each do |contact|%>
        <h2 class='marginbottom'><%=contact.title%></h2>
        <p> <%= contact.street%></p>
        <p><%= contact.zip_code%>, <%=contact.city%></p>
        <p class='marginbottom'><%= contact.country%></p>
        <p><%= contact.phone_number%></p>
        <p><%= contact.email%></p>
        <%=link_to contact.website, "https://#{contact.website}", class:'link', target:'_blank'%>
        <%end%>

      </div>
    </div>
  </div>
</div>
